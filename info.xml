<?xml version="1.0"?>
<extension key="employerlookup" type="module">
  <file>employerlookup</file>
  <name>Profile Current Employer Lookup Field</name>
  <description>Turns current employer field in civicrm profile to an autocomplete lookup field.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Deepak Srivastava</author>
    <email>deepak@mountev.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mountev/employerlookup.git</url>
    <url desc="Documentation">https://github.com/mountev/employerlookup.git</url>
    <url desc="Support">deepak@mountev.co.uk</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-10-06</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Employerlookup</namespace>
  </civix>
</extension>
